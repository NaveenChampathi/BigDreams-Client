    "react": "^16.5.2",
    "react-chartjs-2": "^3.3.0",
    "react-dom": "^16.5.2",
    "react-financial-charts": "^1.3.1",
    "react-router-dom": "^5.2.0",
    "react-stockcharts": "^0.7.8",
    "recharts": "^1.8.5",
    "socket.io-client": "^4.0.2"
  },
